{
  "name": "claudey-ai-browser",
  "version": "1.0.0",
  "description": "AI-powered browser with visual intelligence and multimodal memory",
  "main": "electron-main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "web": "node proxy-server.js",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "cors": "^2.8.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "nodemon": "^3.0.1"
  },
  "build": {
    "appId": "com.algorithmmirror.app",
    "productName": "Algorithm Mirror",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules",
      "!dist"
    ]
  },
  "keywords": [
    "ai",
    "browser",
    "vlm",
    "vision",
    "memory",
    "electron",
    "chromium"
  ],
  "author": "Algorithm Mirror",
  "license": "MIT"
